{
    "id": "talos-worker",
    "name": "talos-worker",
    "boot": {
      "kernel": "/assets/kernel-amd64",
      "initrd": [
        "/assets/initramfs-amd64.xz"
      ],
      "args": [
        "initrd=initramfs-amd64.xz",
        "init_on_alloc=1",
        "slab_nomerge",
        "pti=on",
        "console=tty0",
        "console=ttyS0",
        "printk.devkmsg=on",
        "talos.platform=metal",
        "talos.config=http://10.5.0.8/assets/worker.yaml"
      ]
    }
  }
