{
  "id": "talos-controller",
  "name": "talos-controller",
  "boot": {
    "kernel": "/assets/kernel-arm64",
    "initrd": [
      "/assets/initramfs-arm64.xz"
    ],
    "args": [
      "init_on_alloc=1",
      "slab_nomerge",
      "pti=on",
      "talos.board=rpi_4",
      "talos.platform=metal",
      "talos.shutdown=poweroff",
      "talos.config=http://10.5.0.8/assets/main/controller.yaml"
    ]
  }
}
